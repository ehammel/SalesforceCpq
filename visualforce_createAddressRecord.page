<apex:page standardController="ShippingAccount__c" showHeader="false" sidebar="false" docType="html-5.0" lightningStylesheets="true">
    <apex:form>
        <apex:pageBlock title="Create New Address">
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!ShippingAccount__c.Name}"/>
                <apex:inputField value="{!ShippingAccount__c.Account__c}"/>
                <apex:inputField value="{!ShippingAccount__c.Party__c}"/>
                <apex:inputField value="{!ShippingAccount__c.Locations__c}"/>
                <apex:inputField value="{!ShippingAccount__c.ShipToContact__c}"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons>
                <apex:commandButton action="{!save}" value="Save"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>
